      "optional": true
    },
    "ajv": {
      "optional": true
    }
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.12",
    "@envelop/types": "^5.0.0",
    "@escape.tech/graphql-armor": "^3.2.0",
    "@iggy.rs/sdk": "^1.0.6",
    "@tanstack/query-core": "^5.75.5",
    "@testing-library/react": "^16.3.0",
    "@types/bun": "^1.3.6",
    "@types/react": "^19.1.6",
    "graphile-export": "^1.0.0-rc.0",
    "happy-dom": "^20.5.0",
    "knip": "^5.82.1",
