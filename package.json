{
  "name": "@omnidotdev/providers",
  "version": "0.1.0",
  "description": "Shared auth, authorization, billing, events, feature flags, notifications, storage, and API key providers for the Omni ecosystem",
  "license": "MIT",
  "type": "module",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "types": "./build/index.d.ts"
    },
    "./graphql": {
      "import": "./build/graphql/index.js",
      "types": "./build/graphql/index.d.ts"
    },
    "./react": {
      "import": "./build/react/index.js",
      "types": "./build/react/index.d.ts"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "dev": "bun run build --watch",
    "build": "bun scripts/build.ts",
    "typecheck": "tsc --noEmit",
    "lint": "biome check",
    "format": "biome format --write",
    "knip": "knip-bun",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "ajv": "^8.18.0",
    "jose": "^6.1.3"
  },
  "peerDependencies": {
    "@envelop/types": ">=5.0.0",
    "@escape.tech/graphql-armor": ">=3.0.0",
    "@iggy.rs/sdk": ">=1.0.0",
    "@tanstack/query-core": ">=5.0.0",
    "graphile-export": ">=1.0.0-rc.0",
    "postgraphile": ">=5.0.0-rc.0",
    "react": ">=19.0.0",
    "unleash-client": ">=6.0.0",
    "resend": ">=4.0.0",
    "@aws-sdk/client-s3": ">=3.0.0",
    "@aws-sdk/s3-request-presigner": ">=3.0.0",
    "ajv": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "@envelop/types": {
      "optional": true
    },
    "@escape.tech/graphql-armor": {
      "optional": true
    },
    "@iggy.rs/sdk": {
